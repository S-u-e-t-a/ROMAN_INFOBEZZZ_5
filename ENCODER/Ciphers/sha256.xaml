<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:ciphers="clr-namespace:ENCODER.Ciphers"
                  x:DataType="ciphers:sha256ViewModel"
                  x:Class="ENCODER.Ciphers.sha256">
    <ContentPage.Content>
        <StackLayout>
            <Editor Text="{Binding text}"/>
            <Button BackgroundColor="Fuchsia" Command="{Binding calcHash}"/>
            <Label Text="{Binding hash , Mode=TwoWay}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>